<h1>All Users</h1>

<% @users.each do |user| %>
  <div>
    <%= image_tag(user.image.url(:medium), onerror: "validateImage(event)") %>
  </div>
  <div>
    <%= user.full_name %>
  </div>

    <%= link_to "View Profiles", user_profile_path(user, user.profile) %>
  <hr>
<% end %>

<%= paginate @users %>
