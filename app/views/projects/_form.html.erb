<div class = 'well bs-component'>
  <legend>Edit Project</legend>
  <% @project.errors.full_messages.join(", ") %>
<%= simple_form_for [@user, @profile, @project] do |f| %>
  <%= f.input :title %>
  <%= f.input :link %>
  <%= f.input :github %>
  <%= f.input :description %>
  <%= f.button :submit, class: "btn-primary" %>
<% end %>
